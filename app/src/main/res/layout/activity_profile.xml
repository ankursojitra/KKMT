<?xml version="1.0" encoding="utf-8"?>
<layout>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activity.Profile.Profile">

        <ImageView
            android:id="@+id/img_homeback"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleType="centerCrop"
            android:src="@drawable/backimage"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cnt_tool"
            android:layout_width="0dp"
            android:layout_height="@dimen/_48dp"
            android:layout_marginTop="@dimen/_15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/txt_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Profile"
                android:textColor="@android:color/white"
                android:textSize="@dimen/_14dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/img_back"
                android:layout_width="@dimen/_35dp"
                android:layout_height="@dimen/_35dp"
                android:layout_marginLeft="@dimen/_8dp"
                android:padding="@dimen/_10dp"
                android:src="@drawable/ic_back"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:layout_width="@dimen/_36dp"
                android:layout_height="@dimen/_2dp"
                android:background="@android:color/white"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_title" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="@dimen/_12dp"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout15"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout15">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_4dp"
                    app:cardCornerRadius="@dimen/_9dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent">

                        <ImageView
                            android:id="@+id/imageView6"
                            android:layout_width="@dimen/_75dp"
                            android:layout_height="@dimen/_75dp"
                            android:scaleType="centerCrop"
                            android:src="@drawable/image"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="@dimen/_14dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="@+id/cardView_main"
            app:layout_constraintEnd_toEndOf="@+id/cardView_main"
            app:layout_constraintStart_toStartOf="@+id/cardView_main" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="@dimen/_25dp"
            android:layout_height="@dimen/_25dp"
            android:elevation="@dimen/_8dp"
            app:layout_constraintEnd_toEndOf="@+id/cardView_main"
            app:layout_constraintStart_toStartOf="@+id/cardView_main"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
            app:srcCompat="@drawable/ic_verified" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout15"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_50dp"
            android:background="@drawable/home_back"
            android:paddingTop="@dimen/_54dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cnt_tool">

            <TextView
                android:id="@+id/txt_profile_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/lato_bold"
                android:text="Naria Gonzaleia"
                android:textColor="@android:color/white"
                android:textSize="@dimen/_18dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.core.widget.NestedScrollView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/_8dp"
                android:fillViewport="true"
                android:overScrollMode="never"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_profile_name">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cnt_emp_profile"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_20dp"
                        android:layout_marginTop="@dimen/_2dp"
                        android:layout_marginRight="@dimen/_20dp"
                        android:layout_marginBottom="@dimen/_16dp"
                        app:layout_constraintBottom_toTopOf="@+id/constraintLayou"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/constraintLayout32"
                            android:layout_width="@dimen/_100dp"
                            android:layout_height="match_parent"
                            android:background="@drawable/profile_back"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toEndOf="@+id/constraintLayout33"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/txt_credits"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/lato_bold"
                                android:gravity="center"
                                android:padding="@dimen/_8dp"
                                android:text="12,300"
                                android:textColor="@color/white"
                                android:textSize="@dimen/_11dp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageView19"
                                app:layout_constraintTop_toTopOf="parent" />

                            <ImageView
                                android:id="@+id/imageView19"
                                android:layout_width="@dimen/_20dp"
                                android:layout_height="@dimen/_20dp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toStartOf="@+id/txt_credits"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintHorizontal_chainStyle="packed"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/ic_card" />
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/constraintLayout33"
                            android:layout_width="@dimen/_100dp"
                            android:layout_height="match_parent"
                            android:background="@drawable/profile_back"
                            app:layout_constraintEnd_toStartOf="@+id/constraintLayout32"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/txt_level"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/lato_bold"
                                android:gravity="center"
                                android:padding="@dimen/_8dp"
                                android:text="Level : 12"
                                android:textColor="@color/white"
                                android:textSize="@dimen/_11dp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <TextView
                            android:id="@+id/txt_namevisibility"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_8dp"
                            android:fontFamily="@font/lato_bold"
                            android:gravity="center_vertical"
                            android:padding="@dimen/_8dp"
                            android:text="Name Visibility"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_12dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/constraintLayout33" />

                        <Switch
                            android:id="@+id/switch1"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_10dp"
                            android:background="@drawable/switch_back"
                            android:buttonTint="@android:color/black"
                            android:thumbTint="@color/blue_dark"
                            android:trackTint="@android:color/transparent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/txt_namevisibility"
                            app:layout_constraintTop_toTopOf="@+id/txt_namevisibility" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1dp"
                            android:background="@color/white_8"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="@+id/txt_namevisibility"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayou"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginLeft="@dimen/_20dp"
                        android:layout_marginTop="@dimen/_2dp"
                        android:layout_marginRight="@dimen/_20dp"
                        android:layout_marginBottom="@dimen/_16dp"
                        android:background="@drawable/profile_field"
                        app:layout_constraintBottom_toTopOf="@+id/constraintLayoulname"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/cnt_emp_profile">

                        <EditText
                            android:id="@+id/edt_first_name"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginLeft="@dimen/_4dp"
                            android:background="@android:color/transparent"
                            android:editable="false"
                            android:ems="10"
                            android:enabled="false"
                            android:focusable="false"
                            android:fontFamily="@font/lato_regular"
                            android:gravity="center_vertical"
                            android:inputType="text"
                            android:padding="@dimen/_8dp"
                            android:text="@string/dummy_firstname"
                            android:textColor="@color/prof_gray"
                            android:textSize="@dimen/_14dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageView2"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/imageView2"
                            android:layout_width="@dimen/_16dp"
                            android:layout_height="@dimen/_16dp"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:layout_marginTop="@dimen/_12dp"
                            android:layout_marginBottom="@dimen/_12dp"
                            android:src="@drawable/ic_user"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayoulname"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginBottom="@dimen/_16dp"
                        android:background="@drawable/profile_field"
                        app:layout_constraintBottom_toTopOf="@+id/constraintLayouphonenumber"
                        app:layout_constraintEnd_toEndOf="@+id/constraintLayou"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="@+id/constraintLayou"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayou"
                        app:layout_constraintVertical_bias="0.0">

                        <EditText
                            android:id="@+id/edt_last_name"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginLeft="@dimen/_4dp"
                            android:background="@android:color/transparent"
                            android:editable="false"
                            android:ems="10"
                            android:enabled="false"
                            android:focusable="false"
                            android:fontFamily="@font/lato_regular"
                            android:gravity="center_vertical"
                            android:inputType="text"
                            android:padding="@dimen/_8dp"
                            android:text="@string/dummy_lastname"
                            android:textColor="@color/prof_gray"
                            android:textSize="@dimen/_14dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/img_lname"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/img_lname"
                            android:layout_width="@dimen/_16dp"
                            android:layout_height="@dimen/_16dp"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:layout_marginTop="@dimen/_12dp"
                            android:layout_marginBottom="@dimen/_12dp"
                            android:src="@drawable/ic_user"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayouphonenumber"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginBottom="@dimen/_16dp"
                        android:background="@drawable/profile_field"
                        app:layout_constraintBottom_toTopOf="@+id/constraintLayouemail"
                        app:layout_constraintEnd_toEndOf="@+id/constraintLayoulname"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="@+id/constraintLayoulname"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayoulname"
                        app:layout_constraintVertical_bias="0.0">

                        <EditText
                            android:id="@+id/edt_phone_number"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginLeft="@dimen/_4dp"
                            android:background="@android:color/transparent"
                            android:editable="false"
                            android:ems="10"
                            android:enabled="false"
                            android:focusable="false"
                            android:fontFamily="@font/lato_regular"
                            android:gravity="center_vertical"
                            android:inputType="number"
                            android:padding="@dimen/_8dp"
                            android:text="@string/dummy_phonenumber"
                            android:textColor="@color/prof_gray"
                            android:textSize="@dimen/_14dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/img_phone_number"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/img_phone_number"
                            android:layout_width="@dimen/_16dp"
                            android:layout_height="@dimen/_16dp"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:layout_marginTop="@dimen/_12dp"
                            android:layout_marginBottom="@dimen/_12dp"
                            android:src="@drawable/ic_phonenumber"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayouemail"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginBottom="@dimen/_16dp"
                        android:background="@drawable/profile_field"
                        app:layout_constraintBottom_toTopOf="@+id/constraintLayoudob"
                        app:layout_constraintEnd_toEndOf="@+id/constraintLayouphonenumber"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="@+id/constraintLayouphonenumber"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayouphonenumber"
                        app:layout_constraintVertical_bias="0.0">

                        <EditText
                            android:id="@+id/edt_email"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginLeft="@dimen/_4dp"
                            android:background="@android:color/transparent"
                            android:editable="false"
                            android:ems="10"
                            android:enabled="false"
                            android:focusable="false"
                            android:fontFamily="@font/lato_regular"
                            android:gravity="center_vertical"
                            android:inputType="textEmailAddress"
                            android:padding="@dimen/_8dp"
                            android:text="@string/dummyemail"
                            android:textColor="@color/prof_gray"
                            android:textSize="@dimen/_14dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/img_email"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/img_email"
                            android:layout_width="@dimen/_16dp"
                            android:layout_height="@dimen/_16dp"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:layout_marginTop="@dimen/_12dp"
                            android:layout_marginBottom="@dimen/_12dp"
                            android:src="@drawable/ic_email"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayoudob"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginBottom="@dimen/_16dp"
                        android:background="@drawable/profile_field"
                        app:layout_constraintBottom_toTopOf="@+id/constraintLayougender"
                        app:layout_constraintEnd_toEndOf="@+id/constraintLayouemail"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="@+id/constraintLayouemail"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayouemail"
                        app:layout_constraintVertical_bias="0.0">

                        <EditText
                            android:id="@+id/edt_dob"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginLeft="@dimen/_4dp"
                            android:background="@android:color/transparent"
                            android:editable="false"
                            android:ems="10"
                            android:enabled="false"
                            android:focusable="false"
                            android:fontFamily="@font/lato_regular"
                            android:gravity="center_vertical"
                            android:inputType="textEmailAddress"
                            android:padding="@dimen/_8dp"
                            android:text="@string/date"
                            android:textColor="@color/prof_gray"
                            android:textSize="@dimen/_14dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/img_dob"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/img_dob"
                            android:layout_width="@dimen/_16dp"
                            android:layout_height="@dimen/_16dp"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:layout_marginTop="@dimen/_12dp"
                            android:layout_marginBottom="@dimen/_12dp"
                            android:src="@drawable/ic_dob"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayougender"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginBottom="@dimen/_16dp"
                        android:background="@drawable/profile_field"
                        app:layout_constraintBottom_toTopOf="@id/constraintLayouupload"
                        app:layout_constraintEnd_toEndOf="@+id/constraintLayoudob"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="@+id/constraintLayoudob"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayoudob"
                        app:layout_constraintVertical_bias="0.0">

                        <EditText
                            android:id="@+id/edt_gender"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginLeft="@dimen/_4dp"
                            android:background="@android:color/transparent"
                            android:editable="false"
                            android:ems="10"
                            android:enabled="false"
                            android:focusable="false"
                            android:fontFamily="@font/lato_regular"
                            android:gravity="center_vertical"
                            android:inputType="textEmailAddress"
                            android:padding="@dimen/_8dp"
                            android:text="@string/male"
                            android:textColor="@color/prof_gray"
                            android:textSize="@dimen/_14dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/img_gender"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/img_gender"
                            android:layout_width="@dimen/_16dp"
                            android:layout_height="@dimen/_16dp"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:layout_marginTop="@dimen/_12dp"
                            android:layout_marginBottom="@dimen/_12dp"
                            android:src="@drawable/ic_gender"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayouupload"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginBottom="@dimen/_16dp"
                        android:background="@drawable/numberfield"
                        android:visibility="gone"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/constraintLayougender"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="@+id/constraintLayougender"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayougender"
                        app:layout_constraintVertical_bias="0.0">

                        <TextView
                            android:id="@+id/textView10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:fontFamily="@font/lato_bold"
                            android:text="Upload Documents"
                            android:textColor="@color/blue_dark"
                            android:textSize="@dimen/_14dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView_upload"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_4dp"
                            app:cardCornerRadius="@dimen/_3dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent">


                                <ImageView
                                    android:id="@+id/img_upload"
                                    android:layout_width="@dimen/_40dp"
                                    android:layout_height="@dimen/_40dp"
                                    android:background="@color/blue_dark"
                                    android:padding="@dimen/_12dp"
                                    android:src="@drawable/ic_uparrow"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.core.widget.NestedScrollView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>