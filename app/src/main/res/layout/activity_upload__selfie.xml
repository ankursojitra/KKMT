<?xml version="1.0" encoding="utf-8"?>
<layout>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activity.Register.Upload_Selfie">

        <ImageView
            android:id="@+id/img_back"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleType="centerCrop"
            android:src="@drawable/backimage"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bottomround"
                    android:padding="@dimen/_8dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cnt_upload_selfie"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginLeft="@dimen/_16dp"
                        android:layout_marginTop="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_16dp"
                        android:layout_marginBottom="@dimen/_20dp"
                        android:background="@drawable/numberfield"
                        app:layout_constraintBottom_toTopOf="@+id/cnt_upload_selfiec"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0">

                        <EditText
                            android:id="@+id/edt_phone_num"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:background="@drawable/numberfield"
                            android:ems="10"
                            android:fontFamily="@font/lato_regular"
                            android:gravity="center_vertical"
                            android:hint="@string/Phonenumber"
                            android:inputType="number"
                            android:maxLength="10"
                            android:padding="@dimen/_8dp"
                            android:textSize="@dimen/_14dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/txt_docpath"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:fontFamily="@font/lato_bold"
                            android:text="Upload Selfie"
                            android:textColor="@color/blue_dark"
                            android:textSize="@dimen/_16dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/constraintLayouxc"
                            android:layout_width="@dimen/_40dp"
                            android:layout_height="@dimen/_40dp"
                            android:layout_marginRight="@dimen/_3dp"
                            android:background="@drawable/upload_doc"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <ImageView
                                android:id="@+id/imageView2"
                                android:layout_width="@dimen/_14dp"
                                android:layout_height="@dimen/_14dp"
                                android:src="@drawable/ic_uparrow"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_72dp"
                        android:layout_height="@dimen/_2dp"
                        android:background="@android:color/white"
                        android:orientation="horizontal"
                        app:layout_constraintBottom_toTopOf="@+id/cnt_upload_selfiec"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cnt_upload_selfie" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cnt_upload_selfiec"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_46dp"
                        android:layout_marginLeft="@dimen/_16dp"
                        android:layout_marginTop="@dimen/_20dp"
                        android:layout_marginRight="@dimen/_16dp"
                        android:background="@drawable/numberfield"
                        android:visibility="gone"
                        app:layout_constraintBottom_toTopOf="@+id/textView"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/cnt_upload_selfie"
                        app:layout_constraintVertical_bias="0.0">


                        <!--      <EditText-->
                        <!--          android:id="@+id/edt_phone_num"-->
                        <!--          android:layout_width="0dp"-->
                        <!--          android:layout_height="0dp"-->
                        <!--          android:background="@drawable/numberfield"-->
                        <!--          android:ems="10"-->
                        <!--          android:fontFamily="@font/lato_regular"-->
                        <!--          android:gravity="center_vertical"-->
                        <!--          android:hint="@string/Phonenumber"-->
                        <!--          android:inputType="number"-->
                        <!--          android:maxLength="10"-->
                        <!--          android:padding="@dimen/_8dp"-->
                        <!--          android:textSize="@dimen/_14dp"-->
                        <!--          app:layout_constraintBottom_toBottomOf="parent"-->
                        <!--          app:layout_constraintLeft_toLeftOf="parent"-->
                        <!--          app:layout_constraintTop_toTopOf="parent" />-->

                        <TextView
                            android:id="@+id/txt_selfie"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_16dp"
                            android:fontFamily="@font/lato_bold"
                            android:text="Upload Selfie"
                            android:textColor="@color/blue_dark"
                            android:textSize="@dimen/_16dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/cnt_selfie"
                            android:layout_width="@dimen/_40dp"
                            android:layout_height="@dimen/_40dp"
                            android:layout_marginRight="@dimen/_3dp"
                            android:background="@drawable/upload_doc"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <ImageView
                                android:id="@+id/img_selfie"
                                android:layout_width="@dimen/_14dp"
                                android:layout_height="@dimen/_14dp"
                                android:src="@drawable/ic_uparrow"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10dp"
                        android:fontFamily="@font/lato_medium"
                        android:text="@string/Guidelines"
                        android:textColor="@color/appcolor"
                        android:textSize="@dimen/_20dp"
                        app:layout_constraintBottom_toTopOf="@+id/textView7"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cnt_upload_selfiec" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_18dp"
                        android:layout_marginTop="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_18dp"
                        android:gravity="center"
                        android:text="Take a Selfie holding a Paper with “KKMT Verified” Displayed as Shown Below"
                        android:textColor="@android:color/white"
                        android:textSize="@dimen/_13dp"
                        app:layout_constraintBottom_toTopOf="@+id/textView8"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView" />

                    <ImageView
                        android:id="@+id/textView8"
                        android:layout_width="@dimen/_100dp"
                        android:layout_height="@dimen/_100dp"
                        android:layout_marginTop="@dimen/_10dp"
                        android:gravity="center"
                        android:src="@drawable/kkmt_verified"
                        android:text="@string/docvalidatemsg"
                        android:textColor="@android:color/white"
                        android:textSize="@dimen/_15dp"
                        app:layout_constraintBottom_toTopOf="@+id/constraintLayoutx"
                        app:layout_constraintEnd_toEndOf="@+id/constraintLayoutx"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="@+id/constraintLayoutx"
                        app:layout_constraintTop_toBottomOf="@+id/textView7" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayoutx"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12dp"
                        android:layout_marginBottom="@dimen/_12dp"
                        app:layout_constraintBottom_toTopOf="@+id/txt_skip"
                        app:layout_constraintEnd_toEndOf="@+id/cnt_upload_selfie"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="@+id/cnt_upload_selfie"
                        app:layout_constraintTop_toBottomOf="@+id/textView8">

                        <TextView
                            android:id="@+id/txt_saveandexplore"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/_46dp"
                            android:background="@drawable/proceedback"
                            android:fontFamily="@font/lato_bold"
                            android:gravity="center_vertical"
                            android:paddingLeft="@dimen/_16dp"
                            android:paddingTop="@dimen/_10dp"
                            android:paddingBottom="@dimen/_10dp"
                            android:text="@string/save_expore"
                            android:textColor="@android:color/white"
                            android:textSize="@dimen/_15dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/imageView3"
                            android:layout_width="@dimen/_16dp"
                            android:layout_height="@dimen/_16dp"
                            android:layout_marginTop="@dimen/_12dp"
                            android:layout_marginRight="@dimen/_14dp"
                            android:layout_marginBottom="@dimen/_12dp"
                            android:padding="@dimen/_2dp"
                            android:src="@drawable/ic_preceed"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/txt_saveandexplore"
                            app:layout_constraintTop_toTopOf="parent" />

                        <LinearLayout
                            android:layout_width="@dimen/_1dp"
                            android:layout_height="0dp"
                            android:layout_marginTop="@dimen/_8dp"
                            android:layout_marginRight="@dimen/_14dp"
                            android:layout_marginBottom="@dimen/_8dp"
                            android:background="@drawable/saperator"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/imageView3"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/txt_skip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16dp"
                        android:fontFamily="@font/lato_medium"
                        android:text="@string/Skip"
                        android:textColor="@android:color/white"
                        android:textSize="@dimen/_12dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintHorizontal_chainStyle="packed"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayoutx" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <ImageView
                    android:id="@+id/img_login"
                    android:layout_width="@dimen/_80dp"
                    android:layout_height="@dimen/_80dp"
                    android:layout_marginTop="@dimen/_14dp"
                    android:src="@drawable/ic_document"
                    app:layout_constraintBottom_toTopOf="@+id/textView2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_chainStyle="packed" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_12dp"
                    android:fontFamily="@font/lato_bold"
                    android:gravity="center"
                    android:text="@string/upload_selfie"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/_25dp"
                    app:layout_constraintBottom_toTopOf="@+id/textView3"
                    app:layout_constraintEnd_toEndOf="@+id/img_login"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="@+id/img_login" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_32dp"
                    android:layout_marginRight="@dimen/_32dp"
                    android:layout_marginBottom="@dimen/_12dp"
                    android:fontFamily="@font/lato_medium"
                    android:gravity="center"
                    android:text="Become Verified by Uploading\nSelfie holding a Paper with\n“KKMT Verified”"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/_12dp"
                    app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cnt_upload_doc_type"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@color/dialoug_back"
                    android:visibility="gone"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_36dp"
                        android:layout_marginRight="@dimen/_36dp"
                        app:cardCornerRadius="@dimen/_8dp"
                        app:cardElevation="@dimen/_8dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/textView6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8dp"
                                android:fontFamily="@font/lato_bold"
                                android:text="Document type"
                                android:textColor="@color/blue_dark"
                                android:textSize="@dimen/_10dp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <LinearLayout
                                android:id="@+id/linearLayout"
                                android:layout_width="@dimen/_20dp"
                                android:layout_height="@dimen/_1dp"
                                android:layout_marginTop="@dimen/_4dp"
                                android:background="@color/blue_dark"
                                android:orientation="vertical"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/textView6" />

                            <TextView
                                android:id="@+id/txt_pdf"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:background="@drawable/button_selector"
                                android:fontFamily="@font/lato_bold"
                                android:paddingLeft="@dimen/_16dp"
                                android:paddingTop="@dimen/_8dp"
                                android:paddingBottom="@dimen/_8dp"
                                android:text="PDF"
                                android:textColor="@color/blue_dark"
                                android:textSize="@dimen/_13dp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

                            <LinearLayout
                                android:id="@+id/linearLayout1"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/_1dp"
                                android:layout_marginLeft="@dimen/_16dp"
                                android:layout_marginRight="@dimen/_16dp"
                                android:background="@color/blue_dark_saperator"
                                android:orientation="vertical"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/txt_pdf" />

                            <TextView
                                android:id="@+id/txt_camera"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:background="@drawable/button_selector"
                                android:fontFamily="@font/lato_bold"
                                android:paddingLeft="@dimen/_16dp"
                                android:paddingTop="@dimen/_8dp"
                                android:paddingBottom="@dimen/_8dp"
                                android:text="Camera"
                                android:textColor="@color/blue_dark"
                                android:textSize="@dimen/_13dp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/txt_pdf" />

                            <LinearLayout
                                android:id="@+id/linearLayout2"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/_1dp"
                                android:layout_marginLeft="@dimen/_16dp"
                                android:layout_marginRight="@dimen/_16dp"
                                android:background="@color/blue_dark_saperator"
                                android:orientation="vertical"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/txt_camera" />

                            <TextView
                                android:id="@+id/txt_gallery"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:background="@drawable/button_selector"
                                android:fontFamily="@font/lato_bold"
                                android:paddingLeft="@dimen/_16dp"
                                android:paddingTop="@dimen/_8dp"
                                android:paddingBottom="@dimen/_8dp"
                                android:text="Gallery"
                                android:textColor="@color/blue_dark"
                                android:textSize="@dimen/_13dp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/txt_camera" />

                            <LinearLayout
                                android:id="@+id/linearLayout3"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/_1dp"
                                android:layout_marginLeft="@dimen/_16dp"
                                android:layout_marginRight="@dimen/_16dp"
                                android:background="@color/blue_dark_saperator"
                                android:orientation="vertical"
                                android:visibility="invisible"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/txt_gallery" />

                            <TextView
                                android:id="@+id/txt_cancel"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8dp"
                                android:background="@color/appcolor"
                                android:fontFamily="@font/lato_bold"
                                android:gravity="center"
                                android:paddingTop="@dimen/_8dp"
                                android:paddingBottom="@dimen/_8dp"
                                android:text="Cancel"
                                android:textColor="@android:color/white"
                                android:textSize="@dimen/_13dp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.cardview.widget.CardView>
                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cnt_loader"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/loader_back"
            android:clickable="true"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.cardview.widget.CardView
                android:id="@+id/crd_loader"
                android:layout_width="@dimen/_80dp"
                android:layout_height="@dimen/_80dp"
                android:background="@android:color/transparent"
                app:cardCornerRadius="@dimen/_12dp"
                app:cardElevation="@dimen/_4dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="@dimen/_18dp">

                    <ProgressBar
                        android:id="@+id/progressBar2"
                        style="?android:attr/progressBarStyle"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cnt_alert"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/loader_back"
            android:clickable="true"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.cardview.widget.CardView
                android:id="@+id/crd_alert"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                app:cardCornerRadius="@dimen/_8dp"
                app:cardElevation="@dimen/_4dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent="0.7">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="@dimen/_10dp">

                    <TextView
                        android:id="@+id/txt_otpAlert"
